{
    "name": "dev-paynoomo/zendesk-laravel",
    "type": "library",
    "description": "Laravel wrapper for zendesk/zendesk_api_client_php package",
    "keywords": [
        "zendesk"
    ],
    "homepage": "https://github.com/dev-paynoomo/zendesk-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Huddle Digital Ltd",
            "email": "info@workwithhuddle.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "zendesk/zendesk_api_client_php": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huddle\\Zendesk\\Providers\\ZendeskServiceProvider"
            ],
            "aliases": {
                "Zendesk": "Huddle\\Zendesk\\Facades\\Zendesk"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Huddle\\Zendesk\\": "src"
        }
    },
    "minimum-stability": "stable"
}
